<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Details</th>
      <th>Image</th>
      <th>Street</th>
      <th>City</th>
      <th>Country</th>
      <th>Agent</th>
      <th>Bedroom</th>
      <th>Livingroom</th>
      <th>Parking</th>
      <th>Kitchen</th>
      <th>Featured</th>
      <th>Activated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.title %></td>
        <td><%= listing.price %></td>
        <td><%= listing.details %></td>
        <td><%= listing.image %></td>
        <td><%= listing.street %></td>
        <td><%= listing.city %></td>
        <td><%= listing.country %></td>
        <td><%= listing.agent_id %></td>
        <td><%= listing.bedroom %></td>
        <td><%= listing.livingroom %></td>
        <td><%= listing.parking %></td>
        <td><%= listing.kitchen %></td>
        <td><%= listing.featured %></td>
        <td><%= listing.activated %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
